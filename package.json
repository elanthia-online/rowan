{
  "name": "@elanthia/rowan",
  "version": "0.0.0",
  "description": "a minimal terminal based gui for Gemstone",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "npx ospec",
    "build": "npx tsc",
    "dev": "node --inspect=5858 -r ts-node/register ./src/bin/rowan.ts"
  },
  "bin": "dist/bin/rowan.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elanthia-online/rowan.git"
  },
  "author": "Benjamin Clos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elanthia-online/rowan/issues"
  },
  "homepage": "https://github.com/elanthia-online/rowan#readme",
  "devDependencies": {
    "@types/blessed": "^0.1.10",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/touch": "^3.1.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "ospec": "^3.0.1",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@elanthia/koschei": "git+ssh://git@github.com/elanthia-online/koschei.git",
    "@types/clipboardy": "^1.1.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.8",
    "clipboardy": "^1.2.3",
    "touch": "^3.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  }
}
